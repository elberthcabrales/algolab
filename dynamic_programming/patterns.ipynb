    "\"\"\"\n",
    "GET ALL THE SUB ARRAYS IN A LIST USING A MATRIX\n",
    "\"\"\"\n",
    "nums = [9,6,7,13]\n",
    "n = len(nums)\n",
    "dp = [[0] * n for _ in range(n)]\n",
    "\n",
    "for i in range(n):\n",
    "    product = 1\n",
    "    for j in range(i, n):\n",
    "        product *= nums[j]\n",
    "        dp[i][j] = product\n",
    "\n",
    "print(dp)"
   ]
